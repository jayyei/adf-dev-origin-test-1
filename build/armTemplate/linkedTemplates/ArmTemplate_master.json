{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "adf-dev-origin-test-1"
        },
        "LS_04_ST_01_connectionString": {
            "type": "secureString",
            "metadata": "Secure string for 'connectionString' of 'LS_04_ST_01'"
        },
        "LS_01_KV_01_properties_typeProperties_baseUrl": {
            "type": "secureString",
            "metadata": "Secure string for 'baseUrl' of 'LS_01_KV_01'"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "adf-dev-origin-test-1_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-02-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "LS_04_ST_01_connectionString": {
                        "value": "[parameters('LS_04_ST_01_connectionString')]"
                    },
                    "LS_01_KV_01_properties_typeProperties_baseUrl": {
                        "value": "[parameters('LS_01_KV_01_properties_typeProperties_baseUrl')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}